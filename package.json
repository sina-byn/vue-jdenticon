{
  "name": "vue-jdenticon",
  "version": "1.0.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "license": "MIT",
  "author": "Sina Bayandorian <sina.sina.bayan@protonmail.com> (https://portfolio-sina-byn.vercel.app/)",
  "description": "A simple wrapper around jdenticon library for Vue3",
  "repository": "github:sina-byn/vue-jdenticon",
  "homepage": "https://github.com/sina-byn/vue-jdenticon#readme",
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "jdenticon": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/tsconfig": "^0.7.0",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^2.2.12"
  },
  "bugs": {
    "url": "https://github.com/sina-byn/vue-jdenticon/issues",
    "email": "sina.sina.bayan@protonmail.com"
  },
  "keywords": [
